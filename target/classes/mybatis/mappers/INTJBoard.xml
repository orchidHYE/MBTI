<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
          "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="INTJBoard">
	<!--INTJ게시판목록 전체조회 -->
	<select id="selectINTJBoardList"
		resultType="com.pro.mbti.intj.domain.board.INTJBoardDTO">
			<![CDATA[
				select INTJBoardNo,memberNick,INTJBoardTitle,INTJBoardDate,INTJBoardLike,INTJBoardHit
				from INTJBoard
				order by INTJBoardNo desc
	 		]]>
	</select>

	<insert id="insertINTJBoardList" parameterType="com.pro.mbti.intj.domain.board.INTJBoardDTO" useGeneratedKeys="true" keyProperty="INTJBoardNo">
			<![CDATA[
				insert into INTJBoard(
				memberNick,INTJBoardTitle,INTJBoardDetail,INTJBoardFile,INTJBoardDate,INTJBoardHit,INTJBoardLike)
				values(#{memberNick},#{INTJBoardTitle},#{INTJBoardDetail},#{INTJBoardFile},sysdate(),0,0)
			]]>
	</insert>

</mapper>